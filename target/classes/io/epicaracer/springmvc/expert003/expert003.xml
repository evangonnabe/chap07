<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="koreaTire" class="io.epicaracer.springmvc.expert003.KoreaTire"></bean>

	<bean id="americaTire" class="io.epicaracer.springmvc.expert003.AmericaTire"></bean>

	<bean id="car" class="io.epicaracer.springmvc.expert003.Car">
		<property name="tire" ref="koreaTire"></property></bean>

</beans>